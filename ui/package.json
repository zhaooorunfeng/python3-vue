        "vuedraggable": "^2.19.0",
        "webpack": "^4.46.0",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^4.1.0"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "main": ".eslintrc.js",
    "keywords": [],
    "license": "ISC"
}
