{
    "name": "cw-vue-framework",
    "version": "3.0.2",
    "description": "cw-vue-framework",
    "author": "vue-group",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "npm run dev",
        "stylelint": "npx stylelint ./src/**",
        "lint": "eslint --ext .js,.vue src",
        "clean-dll": "rimraf ../static/dll",
        "build:dll": "webpack --config build/webpack.dll.config.js",
        "build": "npm run clean-dll && npm run build:dll && node build/build.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": "eslint"
    },
    "dependencies": {
        "@antv/data-set": "^0.11.8",
        "@antv/g2": "^4.1.16",
        "axios": "^0.16.2",
        "bk-magic-vue": "latest",
        "echarts": "^5.1.1",
        "jquery": "^2.2.4",
        "lint-staged": "^11.0.0",
        "stylelint": "^13.13.1",
        "stylelint-scss": "^3.19.0",
        "stylelint-webpack-plugin": "^2.2.2",
        "vee-validate": "^2.2.5",
        "vue": "^2.5.2",
        "vue-codemirror": "^4.0.6",
        "vue-i18n": "^8.11.1",
        "vue-json-editor": "^1.3.2",
        "vue-router": "^3.0.1",
        "vuex": "^2.3.1"
    },
    "devDependencies": {
        "@babel/helper-module-imports": "^7.12.13",
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^12.1.4",
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-eslint": "^8.2.1",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-import-bk-magic-vue": "latest",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-plugin-transform-vue-jsx": "^3.5.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "chalk": "^2.0.1",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^2.1.1",
        "eslint": "^4.15.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.2.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-standard": "^3.0.1",
        "eslint-plugin-vue": "^7.12.0",
        "file-loader": "^3.0.1",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.0.0",
        "mini-css-extract-plugin": "^1.6.2",
        "mockjs": "^1.1.0",
        "node-notifier": "^5.1.2",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.8",
        "ora": "^1.2.0",
        "portfinder": "^1.0.13",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.8",
        "postcss-url": "^7.2.1",
